<html>
    <head>
        <script>
            var ws = new WebSocket("/ws");
            ws.onopen = function(evt) {
                console.log("Connection open...");
                ws.send("Hello");
            }
            ws.onmessage = function(evt) {
                console.log(evt.data);
            }

            ws.onclose = function(evt) {
                console.log(1111111);
            }
            
            function sendMessage(data) {
                ws.send(data)
            }
        </script>
    </head>
    <body>
    </body>
</html>
